    <div class="col m3 offset-m1" style="padding: 0 !important; margin-top: 11%;margin-bottom: 0">
        <ul class="collapsible" style="background: white;text-align:center;width: 100%" data-collapsible="accordion">
            <li>
                <div class="collapsible-header">
                    <button data-target="modal1" class="modal-trigger" ng-click="setArr(1)" style="background: none;border: none;">
                        National Steering Committee
                    </button>
                </div>
            </li>
            <li>
                <div class="collapsible-header">
                    <button data-target="modal1" class="modal-trigger" ng-click="setArr(2)" style="background: none;border: none;">National Organizing Committee
                    </button>
                </div>
                <div class="collapsible-body">
                    <div class="row">

                    </div>
                </div>
            </li>
            <li>
                <div class="collapsible-header">
                    <button data-target="modal1" class="modal-trigger" ng-click="setArr(3)" style="background: none;border: none;">
                        International Advisory Committee
                    </button>
                </div>
                <div class="collapsible-body">
                </div>
            </li>
            <li>
                <div class="collapsible-header">
                    <button data-target="modal1" class="modal-trigger" ng-click="setArr(4)" style="background: none;border: none;">

<div id="main_1" class="teal col m2">
    <h1>ICCB '16</h1>
    <hr/>
    <div class="marquee down">
        <p><a href="#!">Announcement 1</a></p>

        <p><a href="#!">Announcement 2</a></p>
    </div>
    <hr/>
    <h3>
        Contact Us
    </h3>

    <table>
        <tr>
            <th>Position</th>
            <th>Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Designation</th>
        </tr>
        <tr ng-repeat="a in contactus">
            <td>{{a.pos}}</td>
            <td>{{a.name}}</td>
            <td>{{a.phone}}</td>
            <td>{{a.email}}</td>
            <td ng-bind-html="a.des"></td>
        </tr>
    </table>

</div>
<script type="text/javascript">
    window.onload = function(){
        if(window.location.href.indexOf("/#/contact") == -1){
            window.location.assign("/#/contact")
        }
    };
</script>
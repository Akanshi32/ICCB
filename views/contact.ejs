<div class="row" style="">
    <div class="col m2">
        <span style="visibility: hidden">asd</span>

    </div>
    <div id="main_1" class=" col m8 slab z-depth-1">

        <h3>
            Contact Us
        </h3>

        <table>
            <tr>
                <th>Position</th>
                <th>Name</th>
                <th>Phone</th>
                <th>Email</th>
                <th>Designation</th>
            </tr>
            <tr ng-repeat="a in contactus">
                <td>{{a.pos}}</td>
                <td>{{a.name}}</td>
                <td>{{a.phone}}</td>
                <td>{{a.email}}</td>
                <td ng-bind-html="a.des"></td>
            </tr>
        </table>

    </div>
</div>
<script type="text/javascript">
    window.onload = function () {
        if (window.location.href.indexOf("/#/contact") == -1) {
            window.location.assign("/#/contact")
        }
    };
</script>